{
  "name": "electron-react-preboot",
  "version": "0.1.0",
  "description": "Electron application boilerplate based on React, React Router, Webpack, React Hot Loader for rapid application development",
  "scripts": {
    "build:dev": "cross-env NODE_ENV=development webpack --profile --color",
    "build:main": "cross-env NODE_ENV=production webpack --profile --color",
    "build:prod": "cross-env NODE_ENV=production webpack --profile --color",
    "build": "npm run clean:build && concurrently \"npm run build:main\" \"npm run build:prod\"",
    "ci": "npm run test",
    "clean:app": "rimraf -- app",
    "clean:build": "npm run clean:app",
    "clean:start": "npm run clean:app && start",
    "clean": "npm cache clean --force && npm run rimraf -- doc coverage public node_modules",
    "electron:dev": "cross-env NODE_ENV=development electron -r ts-node/register ./main.ts",
    "electron:prod": "cross-env NODE_ENV=production electron .",
    "preclean:start": "npm run clean",
    "prepublish": "npm run build",
    "prestart": "npm run clean:app",
    "publish": "build --publish never",
    "server:dev": "cross-env NODE_ENV=development START_ELECTRON=1 webpack-dev-server --profile --color",
    "start": "cross-env NODE_ENV=development npm run server:dev",
    "test": "jest",
    "watch": "cross-env NODE_ENV=development webpack -w"
  },
  "main": "./app/main.bundle.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/katallaxie/electron-react-preboot.git"
  },
  "build": {
    "asar": true,
    "appId": "me.katallaxie.ElectronReactPreboot"
  },
  "author": {
    "name": "Sebastian DÃ¶ll",
    "email": "sebastian@katallaxie.me",
    "url": "https://github.com/katallaxie"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/katallaxie/electron-react-preboot/issues"
  },
  "keywords": [
    "electron",
    "boilerplate",
    "react",
    "redux",
    "webpack",
    "hot",
    "reload"
  ],
  "homepage": "https://github.com/katallaxie/electron-react-preboot#readme",
  "devDependencies": {
    "@types/enzyme": "^3.1.0",
    "@types/jest": "^21.1.0",
