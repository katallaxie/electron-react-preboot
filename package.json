{
  "name": "electron-react-preboot",
  "version": "0.1.0",
  "description": "Electron application boilerplate based on React, React Router, Webpack, React Hot Loader for rapid application development",
  "scripts": {
    "build": "npm run clean:build && concurrently \"npm run build:main\" \"npm run build:prod\"",
    "build:dev": "cross-env NODE_ENV=development node -r babel-register ./node_modules/webpack/bin/webpack --profile --color",
    "build:prod": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --profile --color",
    "build:main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --profile --color",
    "electron:prod": "cross-env NODE_ENV=production electron .",
    "electron:dev": "cross-env NODE_ENV=development electron -r babel-register -r babel-polyfill ./main.js",
    "server:dev": "cross-env NODE_ENV=development START_ELECTRON=1 node --trace-warnings -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --profile --color",
    "clean:build": "npm run clean:app",
    "clean:start": "npm run clean:app && start",
    "clean:app": "rimraf -- app",
    "clean": "npm cache clean --force && npm run rimraf -- doc coverage public aot dll node_modules",
    "preclean:start": "npm run clean",
    "prestart": "npm run clean:app",
    "prepublish": "npm run build",
    "publish": "build --publish never",
    "test": "jest",
    "start": "cross-env NODE_ENV=development npm run server:dev",
    "watch": "cross-env NODE_ENV=development node -r babel-register ./node_modules/webpack/bin/webpack -w"
  },
  "main": "./app/main.bundle.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/katallaxie/electron-react-preboot.git"
  },
  "build": {
    "asar": true,
    "appId": "me.katallaxie.ElectronReactPreboot"
  },
  "author": {
    "name": "Sebastian DÃ¶ll",
