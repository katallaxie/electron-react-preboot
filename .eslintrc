{
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": true,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": ["google", "plugin:react/recommended", "prettier", "prettier", "prettier/react"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": ["error", {
      "singleQuote": true
    }]
  },
  "plugins": ["import", "promise", "compat", "react", "prettier"]
}
