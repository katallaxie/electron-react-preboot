{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [],
  "rules": {
    "semicolon": [
      true,
      "never"
    ],
    "interface-name": false,
    "no-console": [
      true,
      "time",
      "timeEnd",
      "trace"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "no-object-literal-type-assertion": false
  }
}
